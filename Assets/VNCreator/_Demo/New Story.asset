    soundEffect: {fileID: 0}
    backgroundMusic: {fileID: 0}
  - guid: 66063729-8f3c-48d2-83d7-a6e1753100f8
    characterSpr: {fileID: 0}
    characterName: 
    dialogueText: "Me dio mucha risa, pero tambi\xE9n  me sent\xED mal por Julieta
      V\u2026 -la pobre ni siquiera hab\xEDa terminado de escribir su nombre y ya
      hab\xEDa sido bendecida por el atronador ruido de los pedos falsos de Santiago-."
    backgroundSpr: {fileID: 0}
