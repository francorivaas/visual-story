  - guid: 2df636e9-8aad-46f5-a985-fbec0680d52b
    characterSpr: {fileID: 0}
    characterName: 
    dialogueText: "Yo siempre me sentaba al lado de Fran y si bien \xE9l era un hombre
      refinado y de un humor muy caracter\xEDstico, era imposible resistirse a las
      idioteces de Santiago y estall\xF3 tambi\xE9n en carcajadas."
    backgroundSpr: {fileID: 0}
    startNode: 0
    endNode: 0
    choices: 1
