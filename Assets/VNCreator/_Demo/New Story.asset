    soundEffect: {fileID: 0}
    backgroundMusic: {fileID: 0}
  - guid: 7ec42067-789e-42ef-8c77-19f0e7385028
    characterSpr: {fileID: 0}
    characterName: 
    dialogueText: "pero aparentemente \xE9l hab\xEDa o\xEDdo el rumor de que la nueva
      maestra de ingl\xE9s era un fuego y no se lo quiso perder. No lo culpo."
    backgroundSpr: {fileID: 0}
    startNode: 0
    endNode: 0
    choices: 1
    choiceOptions:
