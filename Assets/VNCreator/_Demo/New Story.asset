  - guid: a90f5f3e-d2a8-41e4-8236-edf54752a6e2
    characterSpr: {fileID: 0}
    characterName: Santiago
    dialogueText: "Tengo\u2026 algo\u2026 metido\u2026"
    backgroundSpr: {fileID: 0}
