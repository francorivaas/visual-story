    soundEffect: {fileID: 0}
    backgroundMusic: {fileID: 0}
  - guid: b7c713bb-bbc7-438f-bcc6-91cd0eb2cc6c
    characterSpr: {fileID: 0}
    characterName: 
    dialogueText: "Yo lo hab\xEDa acompa\xF1ado en muchas de sus aventuras fuera
      del colegio, escaparse era muy simple. "
    backgroundSpr: {fileID: 0}
    startNode: 0
    endNode: 0
    choices: 1
    choiceOptions:
    - 
    nodePosition:
      serializedVersion: 2
