  - guid: 34806e8e-62dc-44a9-bc5f-56a8bc745248
    characterSpr: {fileID: 0}
    characterName: 
    dialogueText: "La tem\xE1tica de la conversaci\xF3n era el nuevo libro que Fran
      estaba leyendo."
    backgroundSpr: {fileID: 0}
    startNode: 0
    endNode: 0
    choices: 1
    choiceOptions:
    - 
    nodePosition:
