  - guid: 30c4257a-2b52-4548-bd7a-17056e3431bf
    characterSpr: {fileID: 0}
    characterName: 
    dialogueText: "Ten\xEDamos que estar completamente atentos en esa parte del proceso
      porque es cuando la persona perd\xEDa control total de sus habilidades motoras
      y pod\xEDa partirse la cabeza contra el piso."
    backgroundSpr: {fileID: 0}
    startNode: 0
    endNode: 0
    choices: 1
